<term_definitions code="at0000">
    <items id="text">Organisasjon</items>
    <items id="description">En identifisert formell entitet eller en uformell gruppering av mennesker.</items>
    <items id="comment">For eksempel: Et firma, institusjon, selskap, avdeling, gruppe i nærmiljøet, forsikringsselskap, behandlingsteam eller en gruppe av naboer som opptrer som støtte.</items>
</term_definitions>
<term_definitions code="at0001">
    <items id="text">Navn</items>
    <items id="description">Ustrukturert navn for organisasjonen.</items>
    <items id="comment">For eksempel: "Haukeland Sykehus", "Årstad hjemmesykepleie", "KFUK-KFUM Oslo" elelr "JB Smith advokater".</items>
</term_definitions>
<term_definitions code="at0002">
    <items id="text">Kontaktperson</items>
    <items id="description">Detaljer om en kontaktperson, eller gruppe personer, i en organisasjon.</items>
</term_definitions>
<term_definitions code="at0003">
    <items id="text">Identifikator</items>
    <items id="description">Identifikator for organisasjonen.</items>
</term_definitions>
<term_definitions code="at0004">
    <items id="text">Rolle</items>
    <items id="description">Forholdet eller rollen organisasjonen har til pasienten.</items>
    <items id="comment">For eksempel: Advokat, forsikringsselskap, skole, klinikk, sykehus eller uformelt støttenettverk.</items>
</term_definitions>
<term_definitions code="at0005">
    <items id="text">Adresse</items>
    <items id="description">Detaljer om organisasjonens adresse.</items>
</term_definitions>
<term_definitions code="at0006">
    <items id="text">Elektronisk kommunikasjon</items>
    <items id="description">Detaljer om type elektronisk kommunikasjon.</items>
</term_definitions>
<term_definitions code="at0007">
    <items id="text">Type</items>
    <items id="description">Typen elektronisk kommunikasjon.</items>
    <items id="comment">For eksempel, men er ikke begrenset til: Telefon, mobiltelefon, epost, personsøker, faks, videokonferanseløsning eller sosiale media.</items>
</term_definitions>
<term_definitions code="at0008">
    <items id="text">Mobiltelefon</items>
    <items id="description"></items>
</term_definitions>
<term_definitions code="at0009">
    <items id="text">Telefon (ikke mobiltelefon)</items>
    <items id="description"></items>
</term_definitions>
<term_definitions code="at0010">
    <items id="text">E-post</items>
    <items id="description"></items>
</term_definitions>
<term_definitions code="at0011">
    <items id="text">Personsøker</items>
    <items id="description"></items>
</term_definitions>
<term_definitions code="at0016">
    <items id="text">Verdi</items>
    <items id="description">Den unike kombinasjonen av alfanumeriske tegn, som gir mening for den identifiserte typen elektronisk kommunikasjon.</items>
    <items id="comment">For eksempel fasttelefonnummer, landskode og mobiltelefonnummer, epostadresse eller kontaktnavn på Skype.</items>
</term_definitions>
<term_definitions code="at0017">
    <items id="text">Ytterligere detaljer</items>
    <items id="description">Ytterligere strukturerte detaljer om organisasjonen.</items>
</term_definitions>
<term_definitions code="at0018">
    <items id="text">Kommentar</items>
    <items id="description">Ytterligere fritekst om elektronisk kommunikasjon som ikke er registrert i andre felt.</items>
</term_definitions>
<term_definitions code="at0019">
    <items id="text">Kommentar</items>
    <items id="description">Ytterligere fritekst om personen som ikke er registrert i andre felt.</items>
</term_definitions>
<term_definitions code="at0020">
    <items id="text">Faks</items>
    <items id="description"></items>
</term_definitions>
<term_definitions code="at0021">
    <items id="text">Overordnet organisasjon</items>
    <items id="description">En større organisasjon som denne organisasjonen er en del av.</items>
</term_definitions>
