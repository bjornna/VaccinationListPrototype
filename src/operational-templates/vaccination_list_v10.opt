<term_definitions code="at0001">
    <items id="text">Vaksinatør</items>
    <items id="description">Ustrukturert navn eller label for personen.</items>
    <items id="comment">Innholdet i dette elementet kan være en sammenstilling av en eller flere komponenter fra arketypen CLUSTER.structured_name. For eksempel: "John Markham", "Professor Sir John Markham", "John Markham Jnr MP" eller "Nabo i huset med den røde døren".</items>
</term_definitions>
<term_definitions code="at0002">
    <items id="text">Strukturert navn</items>
    <items id="description">Ytterligere strukturerte detaljer om navnet på en person, som kan kobles sammen til ett felt i dataelementet "Navn".</items>
</term_definitions>
<term_definitions code="at0003">
    <items id="text">Identifikator</items>
    <items id="description">Identifikator for personen.</items>
</term_definitions>
<term_definitions code="at0004">
    <items id="text">Rolle</items>
    <items id="description">Forholdet eller rollen personen har til pasienten.</items>
    <items id="comment">For eksempel: Vitne, medlem i et behandlingsteam, slektning, kontaktperson, nabo eller verge.</items>
</term_definitions>
<term_definitions code="at0005">
    <items id="text">Adresse</items>
    <items id="description">Detaljer om personens adresse.</items>
</term_definitions>
<term_definitions code="at0006">
    <items id="text">Elektronisk kommunikasjon</items>
    <items id="description">Detaljer om type elektronisk kommunikasjon.</items>
</term_definitions>
<term_definitions code="at0007">
    <items id="text">Type</items>
    <items id="description">Typen elektronisk kommunikasjon.</items>
    <items id="comment">For eksempel, men ikke begrenset til: Telefon, mobiltelefon, epost, personsøker, faks, videokonferanseløsning eller sosiale media.</items>
</term_definitions>
<term_definitions code="at0008">
    <items id="text">Mobiltelefon</items>
    <items id="description"></items>
</term_definitions>
<term_definitions code="at0009">
    <items id="text">Telefon (ikke mobiltelefon)</items>
    <items id="description"></items>
</term_definitions>
<term_definitions code="at0010">
    <items id="text">E-post</items>
    <items id="description"></items>
</term_definitions>
<term_definitions code="at0011">
    <items id="text">Personsøker</items>
    <items id="description"></items>
</term_definitions>
<term_definitions code="at0012">
    <items id="text">Bruksområde</items>
    <items id="description">Bruksområde for den identifiserte typen elektronisk kommunikasjon.</items>
    <items id="comment">Koding med ekstern terminologi er ønskelig om mulig.</items>
</term_definitions>
<term_definitions code="at0013">
    <items id="text">Arbeid</items>
    <items id="description"></items>
</term_definitions>
<term_definitions code="at0014">
    <items id="text">Privat</items>
    <items id="description"></items>
</term_definitions>
<term_definitions code="at0015">
    <items id="text">Både arbeid og privat</items>
    <items id="description"></items>
</term_definitions>
<term_definitions code="at0016">
    <items id="text">Verdi</items>
    <items id="description">Den unike kombinasjonen av alfanumeriske tegn, som gir mening for den identifiserte typen elektronisk kommunikasjon.</items>
    <items id="comment">For eksempel fasttelefonnummer, landskode og mobiltelefonnummer, e-postadresse eller skype kontaktnavn.</items>
</term_definitions>
<term_definitions code="at0017">
    <items id="text">Ytterligere detaljer</items>
    <items id="description">Ytterligere detaljer om personen.</items>
</term_definitions>
<term_definitions code="at0018">
    <items id="text">Kommentar</items>
    <items id="description">Ytterligere fritekst om elektronisk kommunikasjon som ikke er registrert i andre felt.</items>
</term_definitions>
<term_definitions code="at0019">
    <items id="text">Kommentar</items>
    <items id="description">Ytterligere fritekst om personen som ikke er registrert i andre felt.</items>
</term_definitions>
<term_definitions code="at0020">
    <items id="text">Organisasjon</items>
    <items id="description">Detaljer om organisasjonstilknytning for denne personen.</items>
    <items id="comment">For eksempel: advokatkontoret som besitter framtidige behandlingsvalg for pasienten, hvor en advokat jobber eller en skole hvor en lærer arbeider.</items>
</term_definitions>
<term_definitions code="at0000">
    <items id="text">Vaksinatør</items>
    <items id="description">Et identifisert menneske.</items>
</term_definitions>
<term_definitions code="VACCINATOR">
    <items id="text">Vaksinatør</items>
</term_definitions>
